      "throw tomaroes": { "file": "/sounds/enchanted.wav" }
    },
    "subscribe": { "file": "/sounds/enchanted.wav" },
    "follow": [
      { "file": "/sounds/enchanted.wav" },
      { "file": "/sounds/follow_sound.wav", "volume": 0.05 }
    ]
  },

  "enabled": {
    "gift": true,
    "subscribe": true,
    "follow": true
  },

  "firstFollowOnly": true,

  "followerGoal": {
    "goal": 50,
    "startingCount": 0
  },

  "giftGoal": {
    "goal": 1000,
    "startingCount": 0
  },

  "gifterRank": {
    "showTotals": true,
    "textColour": "#ffffff",
    "accentColour": "#e3fc02",
    "excludeUsers": []
  },

  "likeRank": {
    "showTotals": true,
    "textColour": "#ffffff",
    "accentColour": "#e3fc02",
    "excludeUsers": []
  },

  "imageSlider": {
    "disappearTime": 5,
    "images": [
      {
        "path": "/images/example1.png",
        "duration": 10
      },
      {
        "path": "/images/example2.png",
        "duration": 15
      }
    ]
  },

  "textScroller": {
    "speed": 2,
    "pauseBetweenMessages": 1,
    "messagesJsonURL": "YOUR_MESSAGES_JSON_URL",
    "accessKey": "YOUR_ACCESS_KEY",
    "masterKey": "YOUR_MASTER_KEY",
    "jsonRefreshIntervalMs": "180000",
    "textColor": "#ffffff",
    "pixelOffColor": "#222"
  },

  "hue": {
    "enabled": false,
    "hubIp": "YOUR_HUE_HUB_IP",
    "username": "YOUR_HUE_USERNAME",
    "clientkey": "YOUR_HUE_CLIENT_KEY",
    "appName": "YOUR_APP_NAME",
    "instanceName": "YOUR_INSTANCE_NAME",
    "targetGroupId": "YOUR_TARGET_GROUP_ID"
  },

  "apiCalls": {
    "apiUrl": "YOUR_API_URL",
    "startStream": {
      "enabled": false,
      "endpoint": "YOUR_START_STREAM_ENDPOINT"
    },
    "endStream": {
      "enabled": false,
      "endpoint": "YOUR_END_STREAM_ENDPOINT"
    },
    "storeLogOffsite": {
      "enabled": false,
      "endpoint": "YOUR_STORE_LOG_ENDPOINT"
    }
  },

  "comments": {
    "enabled": false,
    "maxCommentsStored": 100000,
    "excludeUsers": [],
    "autoRefreshInterval": 5,
    "defaultView": "all" 
  },

  "volume": 0.1
