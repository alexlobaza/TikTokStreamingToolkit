  "fontSize": 25,
  "fontWeight": 800,
  "fadeIn": 2000,
  "fadeOut": 2000,
  "fadeAfter": 5000,
  "textColour": "#ffffff",
  "nameColour": "#32C3A6",

  "sounds": {
    "gift": {
      "default": { "file": "/sounds/fc25_gift.wav" },
      "you're awesome": { "file": "/sounds/enchanted.wav", "volume": 0.2 },
      "10/10": { "file": "/sounds/enchanted.wav" },
